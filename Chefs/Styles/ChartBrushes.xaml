<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <ResourceDictionary.ThemeDictionaries>
        <ResourceDictionary x:Key="Light">
            <SolidColorBrush x:Key="NutritionTrackBackgroundBrush">#1C1B1F14</SolidColorBrush>
            <SolidColorBrush x:Key="DataRiskValBrush">#C12138</SolidColorBrush>
            <SolidColorBrush x:Key="UserRiskValBrush">#EB7726</SolidColorBrush>
            <SolidColorBrush x:Key="DeviceRiskValBrush">#E8C41E</SolidColorBrush>
            <StaticResource x:Key="NutritionDataLabelBrush" ResourceKey="OnSurfaceBrush" />
        </ResourceDictionary>
        <ResourceDictionary x:Key="Dark">
            <SolidColorBrush x:Key="NutritionTrackBackgroundBrush">#242629</SolidColorBrush>
            <SolidColorBrush x:Key="DataRiskValBrush">#C12138</SolidColorBrush>
            <SolidColorBrush x:Key="UserRiskValBrush">#EB7726</SolidColorBrush>
            <SolidColorBrush x:Key="DeviceRiskValBrush">#E8C41E</SolidColorBrush>
            <StaticResource x:Key="NutritionDataLabelBrush" ResourceKey="OnSurfaceBrush" />
        </ResourceDictionary>
    </ResourceDictionary.ThemeDictionaries>
</ResourceDictionary>
